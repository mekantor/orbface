<groupdef id="eq.advanced.content.group" name="Equalizer" inherit_group="wasabi.panel">
	<layer x="0" y="0" w="0" h="0" relatw="1" relath="1"/>
	<layer id="placeholder" x="9" y="3" image="equalizer.placeholder"/>

	<!-- The spline display -->
	<eqvis id="spline" x="80" y="5" w="115" h="15" 
		colortop="0,0,0"
		colormiddle="128,128,128"
		colorbottom="0,0,0"
		colorpreamp="128,128,128" gamma="192"
	/>

	<!-- the different eq switchess -->
	<button
		id="toggle"
		action="EQ_TOGGLE"
		x="10" y="7"
		image="equalizer.switch.on.disabled"
		downImage="equalizer.switch.on.pressed"
		activeImage="equalizer.switch.on.enabled"
		tooltip="Toggle Equalizer On/Off"
	/>

	<button
		id="auto"
		action="EQ_AUTO"
		x="37" y="7"
		image="equalizer.switch.auto.disabled"
		downImage="equalizer.switch.auto.pressed"
		activeImage="equalizer.switch.auto.enabled"
		tooltip="Toggle Equalizer Automatic Processing"
	/>

	<button
		id="presets"
		action="MENU" param="presets"
		x="211" y="7"
		image="equalizer.button.presets"
		downImage="equalizer.button.presets.pressed"
		tooltip="Manage Equalizer Presets"
	/>

	<!-- EQ Sliders -->
	<layer x="19" y="24" image="equalizer.slider.bar"/>
	<slider
		id="preamp"
		action="EQ_PREAMP"
		x="13" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="77" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq1"
		action="EQ_BAND" param="1"
		x="71" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="95" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq2"
		action="EQ_BAND" param="2"
		x="89" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="113" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq3"
		action="EQ_BAND" param="3"
		x="107" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="131" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq4"
		action="EQ_BAND" param="4"
		x="125" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="149" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq5"
		action="EQ_BAND" param="5"
		x="143" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="167" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq6"
		action="EQ_BAND" param="6"
		x="161" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="185" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq7"
		action="EQ_BAND" param="7"
		x="179" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="203" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq8"
		action="EQ_BAND" param="8"
		x="197" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="221" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq9"
		action="EQ_BAND" param="9"
		x="215" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer x="239" y="24" image="equalizer.slider.bar"/>
	<slider
		id="eq10"
		action="EQ_BAND" param="10"
		x="233" y="24"
		w="18" h="64"
		orientation="vertical"
		thumb="equalizer.slider.button"
		downThumb="equalizer.slider.button.pressed"
	/>

	<layer
		id="eq.label.slider.bar"
		x="40" y="100"
		image="equalizer.slider.horizontal.bar.label"
	/>

	<layer
		id="eq.balance.slider.bar"
		x="40" y="106"
		image="equalizer.slider.horizontal.bar"
	/>
	<layer
		id="eq.label.balance.slider"
		x="65" y="116"
		image="equalizer.label.balance"
	/>

	<slider
		id="Balance"
		action="PAN"
		x="39" y="105"
		w="79" h="13"
		thumb="equalizer.slider.horizontal.button"
		downThumb="equalizer.slider.horizontal.button.pressed"
		tooltip="Balance"
	/>

	<layer
		id="eq.label.slider.bar"
		x="149" y="100"
		image="equalizer.slider.horizontal.bar.label"
	/>

	<layer
		id="eq.crossfade.slider.bar"
		x="149" y="106"
		image="equalizer.slider.horizontal.bar"
	/>

	<layer
		id="eq.label.crossfade.slider"
		x="170" y="116"
		image="equalizer.label.crossfade"
	/>

	<slider
		id="Crossfade"
		cfgattrib="{F1239F09-8CC6-4081-8519-C2AE99FCB14C};Crossfade time"
		high="20"
		x="148" y="105"
		w="79" h="13"
		thumb="equalizer.slider.horizontal.button"
		downThumb="equalizer.slider.horizontal.button.pressed"
		tooltip="Crossfade Fade Duration"
	/>
</groupdef>