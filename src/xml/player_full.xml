<layout id="Normal" background="player.main.bgNT">
	<sendparams target="window.titlebar.title" default="WINAMP"/>
	<sendparams target="mousetrap" dblClickAction="SWITCH;shade"/>
	<sendparams  target="window.top.left;window.top;window.top.right;window.left;window.right;window.bottom.left.top;window.bottom.left;window.bottom.left.bottom;window.bottom.top;window.bottom;window.bottom.bottom;window.bottom.right.top;window.bottom.right;window.bottom.right.bottom;" resize="0"/>
	<sendparams target="window.bottom.right.top;window.bottom.right;window.bottom.right.bottom;" scale="bottomright"/>
	<hideobject target="close;close.inactive"/>

	<animatedlayer
		id="previous"
		x="70" y="210" 
		w="31" h="42"
		image="player.button.previous"
		move="0"
		autoplay="0"
		tooltip="Previous"
	/>

	<animatedlayer
		id="pause"
		x="99" y="222" 
		w="31" h="34"
		image="player.button.pause"
		move="0"
		autoplay="0"
		tooltip="Pause"
	/>

	<animatedlayer
		id="play"
		x="128" y="222" 
		w="31" h="34"
		image="player.button.play"
		move="0"
		autoplay="0"
		tooltip="Play"
	/>

	<animatedlayer
		id="next"
		x="157" y="210" 
		w="31" h="42"
		image="player.button.next"
		move="0"
		autoplay="0"
		tooltip="Next"
	/>

	<animatedlayer
		id="stop"
		x="45" y="187" 
		w="27" h="54"
		image="player.button.stop"
		move="0"
		autoplay="0"
		tooltip="Stop"
	/>

	<button 
		id="eject" action="EJECT"
		x="0" y="0"
	/>

	<animatedlayer
		id="open"
		x="27" y="157" 
		w="20" h="66"
		image="player.button.open"
		move="0"
		autoplay="0"
		tooltip="Open File"
	/>

	<button
		id="winshade"
		action="SWITCH" param="shade"
		image="player.button.winshade.normal"
		hoverImage="player.button.winshade.over"
		downImage="player.button.winshade.down"
		w="15" h="22"
		x="153" y="7"
		move="0"
		tooltip="Window Shade"
	/>

	<button
		id="systray"
		action="MINIMIZE"
		image="player.button.systray.normal"
		hoverImage="player.button.systray.over"
		downImage="player.button.systray.down"
		w="15" h="23"
		x="166" y="11"
		tooltip="Minimize"
	/>

	<button
		id="close"
		action="CLOSE"
		image="player.button.close.normal"
		hoverImage="player.button.close.over"
		downImage="player.button.close.down"
		w="15" h="26"
		x="179" y="16"
		move="0"
		tooltip="Exit"
	/>

	<animatedLayer
		id="volume"
		image="player.volume"
		x="2" y="89"
		w="27" h="82"
		move="0"
		autoplay="0"
	/>

	<Layer
		id="player.overlay.1"
		image="player.overlay.1"
		ghost="1"
		x="0" y="0"
	/>

	<vis
		id="vis2"
		x="90" y="208"
		w="80" h="15"
		colorband16="93,192,255"
		colorband15="255,255,255"
		colorband14="255,254,215"
		colorband13="255,252,185"
		colorband12="255,248,170"
		colorband11="255,244,165"
		colorband10="255,240,150"
		colorband9="255,234,135"
		colorband8="255,232,120"
		colorband7="255,230,105"
		colorband6="255,227,90"
		colorband5="255,225,75"
		colorband4="255,222,60"
		colorband3="255,220,45"
		colorband2="255,217,30"
		colorband1="255,207,15"
		colorbandpeak="255,255,255"
		colorosc1="0,0,0"
		colorosc2="0,10,0"
		colorosc3="0,20,0"
		colorosc4="0,30,0"
		colorosc5="0,40,0"
		mode="2"
	/>

	<layer
		id="circle1"
		x="84" y="110"
		image="player.circle.white"
	/>

	<layer
		id="circle2"
		x="133" y="110"
		image="player.circle.white"
	/>

	<layer
		id="blackc1"
		x="102" y="128"
		image="player.circle.black"
	/>

	<layer
		id="blackc2"
		x="151" y="128"
		image="player.circle.black"
	/>

	<text
		id="timer"
		default=""
		display="time"
		x="94" y="21"
		w="60" h="30"
		font="player.number"
		align="right"
	/>

	<text
		id="songticker"
		default=""
		display="songname"
		x="41" y="78"
		w="177" h="12"
		font="player.text"
		align="left"
		ticker="1"
 		showlen="1"
	/>

	<text
		id="songinfo"
		default=""
		display="songinfo"
		x="50" y="65"
		w="160" h="12"
		font="player.text"
		align="left"
		ticker="1"
	/>

	<button
		id="eq"
		action="TOGGLE" param="eq"
		image="player.button.eq.normal"
		hoverImage="player.button.eq.over"
		downImage="player.button.eq.down"
		w="14" h="8"
		x="46" y="186"
		move="0"
		tooltip="Equalizer"
		rectrgn="1"
	/>

	<button
		id="tg"
		action="TOGGLE" param="thinger"
		image="player.button.tg.normal"
		hoverImage="player.button.tg.over"
		downImage="player.button.tg.down"
		w="13" h="10"
		x="68" y="196"
		move="0"
		tooltip="Thinger"
		rectrgn="1"
	/>

	<button
		id="ml"
		action="TOGGLE"
		param="guid:{6B0EDF80-C9A5-11d3-9F26-00C04F39FFC6}"
		image="player.button.ml.normal"
		hoverImage="player.button.ml.over"
		downImage="player.button.ml.down"
		w="14" h="9"
		x="178" y="196"
		move="0"
		tooltip="Media Library"
		rectrgn="1"
	/>

	<button
		id="pl"
		action="TOGGLE"
		param="guid:{45f3f7c1-a6f3-4ee6-a15e-125e92fc3f8d}"
		image="player.button.pl.normal"
		hoverImage="player.button.pl.over"
		downImage="player.button.pl.down"
		w="10" h="11"
		x="200" y="183"
		move="0"
		tooltip="Playlist"
		rectrgn="1"
	/>

	<animatedLayer
		id="volume.bg"
		image="player.volume.bg"
		move="0"
		ghost="1"
		alpha="0"
		autoplay="0"
		w="33" h="33"
		x="17" y="113"
	/>

	<animatedLayer
		id="seekImage"
		image="player.seekImage"
		move="0"
		autoplay="0"
		w="200" h="9"
		x="30" y="94"
	/>

	<togglebutton
		id="crossfadeButton"
		x="95" y="189"
		w="18" h="5"
		image="player.button.crossfade.fake"
		downImage="player.button.crossfade.fake"
		activeImage="player.button.crossfade.fake"
		cfgattrib="{FC3EAF78-C66E-4ED2-A0AA-1494DFCC13FF};Enable crossfading"
	/>

	<animatedLayer
		id="crossfade"
		x="95" y="189"
		w="18" h="5"
		ghost="1"
		image="player.button.crossfade"
		tooltip="Crossfade"
		autoplay="0"
	/>

	<togglebutton
		id="shuffleButton"
		x="115" y="188"
		w="28" h="7"
		image="player.button.shuffle.fake"
		downImage="player.button.shuffle.fake"
		activeImage="player.button.shuffle.fake"
		cfgattrib="{45F3F7C1-A6F3-4EE6-A15E-125E92FC3F8D};Shuffle"
	/>

	<animatedLayer
		id="shuffle"
		x="115" y="188"
		w="28" h="7"
		ghost="1"
		image="player.button.shuffle"
		tooltip="Shuffle"
		autoplay="0"
	/>

	<togglebutton
		id="loopButton"
		x="145" y="188"
		w="17" h="6"
		image="player.button.loop.fake"
		downImage="player.button.loop.fake"
		activeImage="player.button.loop.fake"
		cfgattrib="{45F3F7C1-A6F3-4EE6-A15E-125E92FC3F8D};Repeat"
		cfgval="2"
	/>

	<animatedLayer
		id="loop"
		x="145" y="188"
		w="17" h="6"
		ghost="1"
		image="player.button.loop"
		tooltip="Loop"
		autoplay="0"
	/>
</layout>